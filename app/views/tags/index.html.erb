<h1>Tags</h1>
<table class="table table-bordered">
  <thead>
  <tr>
    <th>Tag</th>
    <th colspan="3"></th>
  </tr>
  </thead>
  <tbody>
  <% @tags.each do |tag| %>
    <%= render(partial: "tag",locals: {tag:tag}) %>
  <% end %>
  <tr id="new_entry"></tr>
  </tbody>
</table>
<%= link_to 'New Tag', new_tag_path ,remote: true,id:"new_tag" %>
