<p id="notice"><%= notice %></p>

<h1>Tag Post Members</h1>

<table>
  <thead>
    <tr>
      <th>Tag</th>
      <th>Post</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tag_post_members.each do |tag_post_member| %>
      <tr>
        <td><%= tag_post_member.tag_id %></td>
        <td><%= tag_post_member.post_id %></td>
        <td><%= link_to 'Show', tag_post_member %></td>
        <td><%= link_to 'Edit', edit_tag_post_member_path(tag_post_member) %></td>
        <td><%= link_to 'Destroy', tag_post_member, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tag Post Member', new_tag_post_member_path %>
