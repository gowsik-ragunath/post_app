$('.pagination').remove();
$('#exist').empty();
$('#generated').empty();
$('#paginate').html('<%= j will_paginate @posts, renderer: BootstrapPagination::Rails %>');
$str = ""
<% @posts.each do |post| %>
  $str += '<%= j render(partial: "posts/post",locals: {post: post}) %>';
<% end %>
$('#generated').html($str);
$('.pagination a').attr('data-remote', 'true');
